{
  "name": "Metalens",
  "manifest_version": 3,
  "version": "0.1.0",
  "description": "Watch your sites metadata (Open Graph, Twitter, Google and more).",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html"
  },
   "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}