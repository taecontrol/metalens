{
  "name": "TaeMeta",
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "Watch your sites metadata.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/content-script.js"
      ]
    }
  ]
}